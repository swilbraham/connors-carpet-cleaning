{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "dev",
      "runtimeExecutable": "node",
      "runtimeArgs": ["-e", "const http=require('http');const fs=require('fs');const path=require('path');const m={'.html':'text/html','.css':'text/css','.js':'application/javascript','.jpg':'image/jpeg','.jpeg':'image/jpeg','.png':'image/png','.svg':'image/svg+xml','.gif':'image/gif','.ico':'image/x-icon'};http.createServer((q,r)=>{let f='.'+(q.url==='/'?'/index.html':q.url);const e=path.extname(f).toLowerCase();fs.readFile(f,(err,c)=>{if(err){r.writeHead(404);r.end('Not found');return;}r.writeHead(200,{'Content-Type':m[e]||'application/octet-stream'});r.end(c);});}).listen(8083,()=>console.log('Server on 8083'));"],
      "port": 8083
    }
  ]
}
